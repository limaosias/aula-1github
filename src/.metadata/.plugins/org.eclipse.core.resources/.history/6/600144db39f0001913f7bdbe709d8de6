package matrizes;

import java.util.Locale;
import java.util.Scanner;

public class Exerc1 {
	public static void main(String[] args){
		Locale.setDefault(Locale.US);
		Scanner sc = new Scanner(System.in);
		int[][] p = new int[10][];
		for(int i = 0; i < 10; i++)
		p[i] = new int[i+1];
		for(int i = 0; i < 10; i++){
		p[i][0] = 1;		
		p[i][0] = 1;
		p[i][i] = 1;
		for(int j = 1; j < i; j++)
		p[i][j] = p[i-1][j-1]+p[i-1][j];
		sc.close();
		}
		}
	
}
